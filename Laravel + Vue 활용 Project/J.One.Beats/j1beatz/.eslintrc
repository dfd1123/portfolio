{
    "extends": [
        "eslint:recommended",
        "plugin:vue/recommended"
    ],
    "parserOptions": {
        "parser": "babel-eslint",
        "sourceType": "module",
        "ecmaVersion": 9,
        "ecmaFeatures": {
            "jsx": false
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jquery": true
    },
    "rules": {
        "vue/html-indent": [
            "error",
            4
        ],
        "indent": [
            "error",
            4
        ],
        "no-trailing-spaces": "error"
    }
}