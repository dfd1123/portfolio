{
  "name": "bcg-client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --modern",
    "lint": "vue-cli-service lint",
    "deploy": "rsync -ahv dist/* deploy"
  },
  "gitHooks": {
    "post-merge": "npm install && npm run build && npm run deploy"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^18.4.2",
    "blueimp-load-image": "^2.24.0",
    "compressorjs": "^1.0.6",
    "core-js": "^3.4.5",
    "element-ui": "^2.13.0",
    "lodash-es": "^4.17.15",
    "moment": "^2.24.0",
    "vue": "^2.6.10",
    "vue-doughnut-chart": "^0.1.0",
    "vue-img-orientation-changer": "^1.0.11",
    "vue-jpeg-auto-rotation": "^1.0.1",
    "vue-momentjs": "^0.1.2",
    "vue-router": "^3.1.3",
    "vue-sweetalert2": "^2.1.5",
    "vue-textarea-autosize": "^1.1.1",
    "vue-timeselector": "^1.0.0",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.1",
    "@vue/cli-plugin-eslint": "^4.1.1",
    "@vue/cli-plugin-router": "^4.1.1",
    "@vue/cli-service": "^4.1.1",
    "@vue/eslint-config-standard": "^5.0.0",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "scss-loader": "0.0.1",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-cli-plugin-moment": "^0.1.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaVersion": 2019
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "plugins": [
      "vue"
    ],
    "rules": {}
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "defaults",
    "not ie <= 11"
  ]
}
